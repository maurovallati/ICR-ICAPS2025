(define (problem delivery-x-1)
	(:domain delivery)
	(:objects
		rooma roomb roomc - room
		item4 item3 item2 item1 - item
		bot1 bot2 - bot
		left1 right1 left2 right2 - arm
	)
	(:init
		(free right1)
		(= (weight item3) 1.0)
		(at item1 rooma)
		(= (weight item4) 1.0)
		(= (current_load bot1) 0.0)
		(mount left2 bot2)
		(= (weight item1) 1.0)
		(door rooma roomb)
		(free right2)
		(= (load_limit bot1) 4.0)
		(= (load_limit bot2) 4.0)
		(at item3 rooma)
		(mount right2 bot2)
		(free left2)
		(at-bot bot2 rooma)
		(at-bot bot1 rooma)
		(free left1)
		(at item4 rooma)
		(mount right1 bot1)
		(= (current_load bot2) 0.0)
		(mount left1 bot1)
		(door rooma roomc)
		(door roomb rooma)
		(at item2 rooma)
		(= (weight item2) 1.0)
		(door roomc rooma)
		(= (cost) 0.0)
	)
	(:goal
			(and
				(at item4 roomb)
				(at item3 roomb)
				(at item2 roomc)
				(at item1 roomc)
			)
	)
)